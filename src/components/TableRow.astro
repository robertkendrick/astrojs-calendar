---
import { string } from "astro:schema";
import "../styles/global.css"; 
// import {format} from 'date-fns'

const {row} = Astro.props
const rowValues:any = Object.values(row)    //array with just the values
// console.log('rowValues: ', rowValues)
const date = new Date(rowValues[0])
// const formDate = format(date, "eee dd MMM")

const dateItems = date.toString().split(' ')    // split date to array of each item


rowValues.shift()

// split first and last names in arrays
const tt = rowValues.map( (item: string) => {
                // console.log(item)
                const us = item.split(' ') 
                // item = '<span>us[0]</span>is[1]<span>'
                // item = us[0] + '<br>' + us[1]
                return us
})
    
    // <td class="tdcell">username=userName[0] + '<br />' + userName[1]</td>


console.log('TableRow-tt: ', tt)
---


<tr class="row">

            <!-- <th class="thstyle">{formDate}</th>      -->
            <th class="thstyle">{dateItems[0]}<br>{dateItems[2]}<br>{dateItems[1]}</th>
            {    
                tt.map( (item: []) => <td class="tdcell"><span>{item[0]}<br></span><span>{item[1]}</span></td> )
            }
</tr>


